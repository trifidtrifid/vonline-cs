<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite>
    <rule>
        <from>^/(null|profile/.*|blog|main|wall-single/.*|talks|talks-single/.*|profit|profit-single/.*|dialogs|dialog-single/.*|neighbours|profile.change-avatar|settings|maps|set-info|counters|counters-history/.*|important)$</from>
        <to>/main.html</to>
    </rule>
    <rule>
        <from>/login</from>
        <to>/login.html</to>
    </rule>
    <rule>
        <from>/about</from>
        <to>/about.html</to>
    </rule>
    <rule>
        <from>/contacts</from>
        <to>/contacts.html</to>
    </rule>
    <rule>
        <from>/login.jsp</from>
        <to>/login.html</to>
    </rule>
</urlrewrite>