{
  "name": "VO",
  "version": "0.1.0",
  "description": "Социальная сеть для соседей",
  "main": "static/js/app.js",
  "scripts": {
    "build": "browserify static/js/app.js -o static/js/app-build.js",
    "watch": "watchify static/js/app.js -o static/js/app-build.js --debug --verbose",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "depends": "jquery",
      "exports": "angular"
    }
  },
  "author": "VO",
  "license": "Commercial",
  "dependencies": {
    "angular": "1.2",
    "angular-bootstrap": "0.12",
    "angular-ui-router": "0.2",
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "jquery": "^2.1.3",
    "lodash-node": "^3.1.0"
  }
}
